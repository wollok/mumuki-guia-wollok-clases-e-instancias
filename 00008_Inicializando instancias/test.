	test "La energía inicial de Juliana es 1000" {
		assert.equals(1000, juliana.energia())
	}

	test "La energía inicial de Anastasia es 150" {
		assert.equals(150, anastasia.energia())
	}

	test "Si Juliana ataca con 5 puntos de daño a Bouba su salud disminuye en 10 puntos" {
		juliana.atacar(bouba, 5)
		assert.equals(90, bouba.salud())
	}

	test "Si Anastasia ataca con 5 puntos de daño a Kiki, su salud disminuye en 5 puntos, porque está cansada" {
		anastasia.atacar(kiki, 5)
		assert.equals(95, bouba.salud())
	}